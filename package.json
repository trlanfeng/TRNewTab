{
  "name": "trnewtab",
  "version": "2.2.2",
  "description": "开源、简洁、快速、易用、可同步、无广告的新标签页。",
  "main": "index.js",
  "repository": "https://gitee.com/trlanfeng/TRNewTab.git",
  "author": "孤月蓝风 <trlanfeng@foxmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "parcel ./src/newtab.html ./src/popup.html ./src/background.js",
    "build": "rm -rf ./dist && parcel build ./src/newtab.html ./src/popup.html ./src/background.js && cp -rf ./template/* ./dist",
    "build-windows": "rmdir dist /s /q && parcel build ./src/newtab.html ./src/popup.html ./src/background.js && xcopy .\\template\\* .\\dist /S /Y"
  },
  "dependencies": {
    "@babel/preset-env": "^7.7.6",
    "assets": "^3.0.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4",
    "vue-slider-component": "^2.7.8",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@vue/component-compiler-utils": "^2.1.2",
    "cssnano": "^4.1.4",
    "less": "^3.10.3",
    "typescript": "^3.7.3",
    "vue-template-compiler": "^2.6.11"
  }
}
